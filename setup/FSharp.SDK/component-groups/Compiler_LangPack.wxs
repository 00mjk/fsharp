<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi" xmlns:NetFx="http://schemas.microsoft.com/wix/NetFxExtension">

	<Fragment>
		<ComponentGroup Id="Compiler_LangPack">

			<ComponentRef Id="Compiler_LangPack_FSharp.Core.dll_$(var.Lang)" />
			<ComponentRef Id="Compiler_LangPack_FSharp.Build.dll_$(var.Lang)" />
			<ComponentRef Id="Compiler_LangPack_FSharp.Compiler.dll_$(var.Lang)" />
			<ComponentRef Id="Compiler_LangPack_fsc.exe_$(var.Lang)" />
			<ComponentRef Id="Compiler_LangPack_fsi.exe_$(var.Lang)" />
			<ComponentRef Id="Compiler_LangPack_fsiAnyCPU.exe_$(var.Lang)" />
			
		</ComponentGroup>

		<DirectoryRef Id="MicrosoftSDKs_FS_4.1_Framework_v4.0">

            <Component Id="Compiler_LangPack_FSharp.Core.dll_$(var.Lang)" Guid="$(fsharp.guid(Compiler_LangPack_FSharp.Core.dll_$(var.Lang), $(var.Lang)))">
                <File Id="Compiler_LangPack_FSharp.Core.dll_$(var.Lang)" Source="$(var.BinariesDir)\net40\bin\localize\$(var.Lang)\FSharp.Core.resources.dll" KeyPath="yes" />
            </Component>
            
            <Component Id="Compiler_LangPack_FSharp.Build.dll_$(var.Lang)" Guid="$(fsharp.guid(Compiler_LangPack_FSharp.Build.dll_$(var.Lang), $(var.Lang)))">
                <File Id="Compiler_LangPack_FSharp.Build.dll_$(var.Lang)" Source="$(var.BinariesDir)\net40\bin\localize\$(var.Lang)\FSharp.Build.resources.dll" KeyPath="yes" />
            </Component>
            
            <Component Id="Compiler_LangPack_FSharp.Compiler.dll_$(var.Lang)" Guid="$(fsharp.guid(Compiler_LangPack_FSharp.Compiler.dll_$(var.Lang), $(var.Lang)))">
                <File Id="Compiler_LangPack_FSharp.Compiler.dll_$(var.Lang)" Source="$(var.BinariesDir)\net40\bin\localize\$(var.Lang)\FSharp.Compiler.resources.dll" KeyPath="yes" />
            </Component>
            
            <Component Id="Compiler_LangPack_fsc.exe_$(var.Lang)" Guid="$(fsharp.guid(Compiler_LangPack_fsc.exe_$(var.Lang), $(var.Lang)))">
                <File Id="Compiler_LangPack_fsc.exe_$(var.Lang)" Source="$(var.BinariesDir)\net40\bin\localize\$(var.Lang)\fsc.resources.dll" KeyPath="yes" />
            </Component>
            
            <Component Id="Compiler_LangPack_fsi.exe_$(var.Lang)" Guid="$(fsharp.guid(Compiler_LangPack_fsi.exe_$(var.Lang), $(var.Lang)))">
                <File Id="Compiler_LangPack_fsi.exe_$(var.Lang)" Source="$(var.BinariesDir)\net40\bin\localize\$(var.Lang)\fsi.resources.dll" KeyPath="yes"/>
            </Component>
            
            <Component Id="Compiler_LangPack_fsiAnyCPU.exe_$(var.Lang)" Guid="$(fsharp.guid(Compiler_LangPack_fsiAnyCPU.exe_$(var.Lang), $(var.Lang)))">
                <File Id="Compiler_LangPack_fsiAnyCPU.exe_$(var.Lang)" Source="$(var.BinariesDir)\net40\bin\localize\$(var.Lang)\fsiAnyCPU.resources.dll" KeyPath="yes" />
            </Component>
            
		</DirectoryRef>
	</Fragment>
</Wix>